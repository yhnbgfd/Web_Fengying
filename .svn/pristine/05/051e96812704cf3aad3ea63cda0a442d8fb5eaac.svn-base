function onload(){
	getParameter();
}

function getParameter(){
	var s=location.search;
	var para = s.substr(-4);
	rightframe(para);
}

function rightframe(str){
	var obj = document.getElementById("rightframe");
	var tle = document.getElementById("righttitle");
	SetHeight();
	if(str=="news"){
		tle.innerHTML="新闻中心 > 行业新闻";
		obj.src="content/news/hangye/index.html";
	}else if(str=="hyxw"){	//行业新闻
		tle.innerHTML="新闻中心 > <a href=\"#\" style=\"color:#000000;\" onclick=\"javascript:rightframe('hyxw')\">行业新闻</a>";
		obj.src="content/news/hangye/index.html";
	}else if(str=="gsxw"){	//公司新闻
		tle.innerHTML="新闻中心 > <a href=\"#\" style=\"color:#000000;\" onclick=\"javascript:rightframe('gsxw')\">公司新闻</a>";
		obj.src="content/news/fy2000/index.html";
	}else if(str=="prod"){
		tle.innerHTML="产品服务";
		obj.src="content/about.html";
	}else if(str=="solu"){
		tle.innerHTML="解决方案";
		obj.src="content/about.html";
	}else if(str=="abou"){
		tle.innerHTML="关于丰颖";
		obj.src="content/about.html";
	}else if(str=="cont"){
		tle.innerHTML="联系我们";
		obj.src="content/contact.html";
	}
}

function SetHeight(){ 
	var win=document.getElementById("rightframe"); 
	if (document.getElementById) {
		if (win.contentDocument && win.contentDocument.body.offsetHeight) {
			win.height = win.contentDocument.body.offsetHeight+100;
		}else if(win.Document && win.Document.body.scrollHeight){
			win.height = win.Document.body.scrollHeight+100;
		}
		if(win.height < 400){
			win.height =400;
		}
	}
}
window.setInterval( "SetHeight()",200);
</script>
